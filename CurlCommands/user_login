        <script type="text/javascript">











            function deleteItem(msg)



            {



                if (confirm(msg))



                    return true;



                else



                    return false;



            }



            function Confirm_msg(msg, shwID, focusId)



            {



                $("#" + shwID).empty();



                $("#" + shwID).append('<span style="color:#FF0000; font-weight:bold"><img src="http://software.ipvision.ca/images/unchecked.gif" > ' + msg + '</span>');



                if (focusId != '') {



                    $("#" + focusId).focus();



                    $("#" + focusId).select();



                }



                return (false);



            }



            function Confirm_msg_for_cust(msg, shwID, focusId)



            {



                $("#" + shwID).empty();



                $("#" + shwID).append('<span style="color:#FF0000; font-weight:bold">' + msg + '</span>');



                if (focusId != '') {



                    $("#" + focusId).focus();



                    $("#" + focusId).select();



                }



                return (false);



            }



            function showAll_CI(allvalue, s_inptID, msgg, formId, ErrormsgId)



            {



                var baseUrl = 'http://software.ipvision.ca/';



                if (allvalue == 200 && document.getElementById(s_inptID).value == '') {



                    //document.getElementById(ErrormsgId).innerHTML='<img src="'+baseUrl+'images/unchecked.gif" alt="verify" /><span style="color: #FF0000; font-weight:bold;"> '+msgg+'</span>';



                    alert(msgg);



                    document.getElementById(s_inptID).focus();



                    return false;



                }



                else {



                    document.getElementById(ErrormsgId).innerHTML = '&nbsp;';



                }



                if (allvalue == 200) {



                    document.getElementById("s_sall").value = document.getElementById(s_inptID).value;



                }



                else if (allvalue == 100) {



                    document.getElementById("s_sall").value = 9999999999;



                }



                document.getElementById(formId).submit();



            }



            function calcJulian(isDate)



            {



                gregDate = new Date(isDate);



                year = gregDate.getFullYear();



                month = gregDate.getMonth() + 1;



                day = gregDate.getDate();



                A = Math.floor((7 * (year + Math.floor((month + 9) / 12))) / 4);



                B = day + Math.floor((275 * month) / 9)



                isJulian = (367 * year) - A + B + 1721014;



                return isJulian;



            }



            function birth_DATE_CHEK(prev_Date, Big_Date, Whatt)



            {



                tmp = prev_Date.split("-")



                xDate = tmp[1] + "-" + tmp[0] + "-" + tmp[2];



                pr_refDate = calcJulian(xDate);



                tmp = Big_Date.split("/")



                xDate = tmp[1] + "-" + tmp[0] + "-" + tmp[2];



                B_fwdDate = calcJulian(xDate);







                if (Whatt == 1) {



                    return (pr_refDate - B_fwdDate);



                }



            }



            function isDecimalValidKey(textboxIn, evt)



            {



                var charCode = (evt.which) ? evt.which : event.keyCode;



                if (charCode > 31 && (charCode < 45 || charCode == 47 || charCode > 57))



                {



                    return false;



                }



                if (charCode == 46 && textboxIn.value.indexOf(".") != -1)



                {



                    return false;



                }



                return true;



            }
















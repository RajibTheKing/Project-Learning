<script type="text/javascript">
    $( document ).ready(function() {

        $('#meal-table input[type="radio"]').click(meal_status);

        function meal_status(){
            var value = $(this).val();
            var meal_name = $(this).attr('name');
            var date = $(this).data('date');
            if(meal_name.indexOf('lunch')!=-1) {
                var lunch_yes = $(this).closest('tr').find('input.meal_item1');
                if(value==1) {
                    lunch_yes.prop("checked", true);
                } else if(value==0) {
                    lunch_yes.prop("checked", false);
                    var item = lunch_yes.parent().next('td').find('input');
                    item.prop("checked", false);
                }
              }

            if(meal_name.indexOf('meal')!=-1){
                var meal_item = $(this).closest('tr').find('input.meal_item2');
                if(value>0) {
                    meal_item.prop("checked", true);
                }
            }
            $.ajax({
                url: "http://software.ipvision.ca/index.php/login/meal_submit/" + value + "/" + date + "/" + meal_name,
                type:"post",
                success: function (result) {
                    if(result == 2){
                        $('#meal-msg p').remove();
                        $('#meal-msg').append('<p>Meal status updated successfully</p>').fadeIn(400);
                    }else if(result == 1){
                        $('#meal-msg p').remove();
                           $('#meal-msg').append('<p>Meal submitted successfully</p>').fadeIn(400);
                    }else if(result == 0){
                        window.location.href = "http://software.ipvision.ca/index.php/login";
                    }
                    $('#meal-msg').fadeOut(3000);

                }
            });
        }
    });

</script>

